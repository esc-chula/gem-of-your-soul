---
import ImagePageLayout from "@/layouts/ImagePageLayout.astro";

import Tap from "@/components/Tap";

import BackgroundImage from "@/assets/story/scene-0/0-03.webp";
import BackgroundImageLow from "@/assets/story/scene-0/0-03-low.webp";
---

<ImagePageLayout
  backgroundColor="bg-[#303030]"
  image={BackgroundImage}
  imageLowRes={BackgroundImageLow}
>
  <Tap delay={0.5} redirectUrl="/story/0-03" client:load />
</ImagePageLayout>
